package com.rd.client.common.action;

import com.smartgwt.client.widgets.grid.ListGridRecord;

public class SkuClsRecord extends ListGridRecord {

    public SkuClsRecord() {
    }

    public SkuClsRecord(String descr_c) {

    	setDESCR_C(descr_c);
    }

    public void setDESCR_C(String continent) {
        setAttribute("DESCR_C", continent);
    }

    public String getDESCR_C() {
        return getAttributeAsString("DESCR_C");
    }


    public String getFieldValue(String field) {
        return getAttributeAsString(field);
    }
}
