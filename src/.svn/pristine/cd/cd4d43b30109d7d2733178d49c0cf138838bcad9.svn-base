<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.rd.client.common.obj.FUNCTION" table="SYS_FUNCTION">
        <id name="FUNCTION_ID">
            <generator class="native"/>
        </id>
        <property name="FUNCTION_NAME" type="string"></property>
		<property name="PARENT_FUNCTION_ID" type="string"></property>
		<property name="FUNCTION_FORMNAME" type="string"></property>
    </class>
</hibernate-mapping>